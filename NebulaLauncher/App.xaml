<Application x:Class="NebulaLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:NebulaLauncher">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!-- Order is important -->
                <ResourceDictionary Source="Styles/GameAppStyle.xaml"/>
                <ResourceDictionary Source="Styles/BaseWidgetsStyle.xaml"/>
                <ResourceDictionary Source="Styles/TitleBarButtonStyles.xaml"/>
                <ResourceDictionary Source="Styles/NebulaWindowStyle.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
